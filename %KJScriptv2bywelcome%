-- Navigate to "Weakest Dummy" inside Workspace > Live
local liveFolder = Workspace:FindFirstChild("Live")
local weakestDummy = liveFolder and liveFolder:FindFirstChild("Weakest Dummy")

if not weakestDummy then
    error("Weakest Dummy model not found in Workspace.Live")
end

-- Find the "Torso" part inside "Weakest Dummy"
local torso = weakestDummy:FindFirstChild("Torso")
if not torso then
    error("Torso part not found in Weakest Dummy")
end

-- Check for Resources folder in ReplicatedStorage
local resourcesFolder = ReplicatedStorage:WaitForChild("Resources")

-- Check for KJEffects folder inside Resources
local kjEffectsFolder = resourcesFolder:WaitForChild("KJEffects")

-- Check for speedlinesandstuff part inside KJEffects
local speedlinesandstuffPart = kjEffectsFolder:WaitForChild("barrage")

-- Duplicate the speedlinesandstuff part
local speedlinesandstuffClone = speedlinesandstuffPart:Clone()

-- Calculate the desired CFrame using the "Torso" position
local desiredCFrame = CFrame.new(torso.Position)

-- Set the desired CFrame for the clone
speedlinesandstuffClone.CFrame = desiredCFrame

-- Put the duplicate in Workspace
speedlinesandstuffClone.Parent = Workspace

-- Function to enable all ParticleEmitters
local function enableParticleEmitters(parent)
    for _, descendant in ipairs(parent:GetDescendants()) do
        if descendant:IsA("ParticleEmitter") then
            descendant.Enabled = true
        end
    end
end

-- Example usage after your dash effect completes
spawn(function()
    -- Simulating end of dash effect
    wait(0)  -- Adjust the wait time as needed

    -- Enable all ParticleEmitters inside speedlinesandstuffClone
    enableParticleEmitters(speedlinesandstuffClone)
end)
wait(2)
workspace.barrage:Destroy()
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local RunService = game:GetService("RunService")

local function createSpinningRocks(character)
    local rocks = {}

    for i = 1, 5 do
        local rock = Instance.new("Part")
        rock.Size = Vector3.new(0.5, 0.5, 0.5)
        rock.Shape = Enum.PartType.Block
        rock.Anchored = true
        rock.CanCollide = false
        rock.Color = Color3.fromRGB(128, 128, 128) -- Grey color
        rock.Material = Enum.Material.Slate -- Rock texture
        rock.Parent = workspace

        table.insert(rocks, rock)
    end

    local startTime = tick()
    local duration = 0.7 -- Duration in seconds
    local radius = 5 -- Radius of the circle
    local rotationSpeed = 2 * math.pi -- Full rotation in 1 second

    -- Update function to spin the rocks
    local connection
    connection = RunService.RenderStepped:Connect(function()
        local elapsedTime = tick() - startTime
        local angle = elapsedTime * rotationSpeed
        local angleStep = 2 * math.pi / #rocks

        for index, rock in ipairs(rocks) do
            local rockAngle = angle + index * angleStep
            local offsetX = math.cos(rockAngle) * radius
            local offsetZ = math.sin(rockAngle) * radius
            rock.Position = character.PrimaryPart.Position + Vector3.new(offsetX, 2, offsetZ)
        end

        -- Destroy the rocks after the duration is over
        if elapsedTime >= duration then
            for _, rock in ipairs(rocks) do
                rock:Destroy()
            end
            connection:Disconnect() -- Stop updating after destruction
        end
    end)
end

-- Call the function to create spinning rocks
createSpinningRocks(character)

-- Update the function if the player's character changes
player.CharacterAdded:Connect(function(newCharacter)
    character = newCharacter
    createSpinningRocks(newCharacter)
end)
